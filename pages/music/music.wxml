<!--pages/music/music.wxml-->
<view class='playView'>
    <swiper  current="0" interval="5000" duration="1000" bindchange="" style="height:100%">
        <block wx:for="{{musics}}">
            <swiper-item>
                <video src="{{baseURL+item.address}}" controls style="width:100%;height:100%;opacity:0"></video>
                <button bindtap="play">play</button>
            </swiper-item>
        </block>
    </swiper>
</view>
