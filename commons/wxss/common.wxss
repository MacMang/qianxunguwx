/*顶部导航条*/
.topBar{
    position:fixed;
    width:100%;
    height:64px;
    background:rgba(0,0,0,0);
    z-index:1000;
    opacity:0;
    top: 0;
}
.statuBar{
    height:20px;
    width:100%;
}

.navBar {
    width:100%;
    height: 44px;
    display:flex;
}
.navBar>view{
    flex:2;
    text-align:left;
    display:flex;
    align-items:center;
}

.CancelText {
    padding-left:20px;
    box-sizing:border-box;
    color:white;
}

@media  screen and (min-height:812px) and (max-height:1000px){
    .topBar {
        height:84px;
    }
    .statuBar{
        height:40px;
    }

}